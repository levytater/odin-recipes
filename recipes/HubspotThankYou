  <script>
  document.addEventListener('DOMContentLoaded', () => {
    
    var emailFill = localStorage.getItem("emaillabel");  
    var url = document.getElementById("A tag ID Here").href; //<a> tag ID here
    var decodedEmail =  atob(emailFill);
    
if (url.indexOf('?') > -1){
  document.getElementById("pflink").href = url + '&lb-mode=overlay&lb_email=' + decodedEmail //<a> tag ID here
}else{
  document.getElementById("pflink").href = url + '?lb-mode=overlay&lb_email=' + decodedEmail //<a> tag ID here
}
localStorage.removeItem("emaillabel");
    });
   
</script>



